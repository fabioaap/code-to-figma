# E2E Test Environment Configuration
# Copy this file to .env for local development

# Base URL for the Storybook instance
# Default: http://localhost:6006
E2E_BASE_URL=http://localhost:6006

# Run tests in headless mode (true) or with visible browser (false)
# Default: true
E2E_HEADLESS=true

# Timeout for each test in milliseconds
# Default: 30000 (30 seconds)
E2E_TIMEOUT=30000

# CI mode - automatically set by CI environments
# Default: false
CI=false
